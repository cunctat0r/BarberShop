<table class="table table-hover">
	<thead>
		<tr>
			<th> Имя </th>
			<th> Телефон </th>
			<th> Дата </th>
			<th> Мастер </th>
			<th> Цвет </th>
		</tr>
	</thead>
	<tbody>
		<% @results.each do |row| %>
			<tr>
				<td><%= row['username'] %></td>
				<td><%= row['phone'] %></td>
				<td><%= row['datestamp'] %></td>
				<td><%= row['barber'] %></td>
				<td><%= row['color'] %></td>
			</tr>
		<% end %>	
	</tbody>
</table>